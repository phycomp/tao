var xhr=new XMLHttpRequest();
xhr.data_sent=function(url, data){
	this.open('POST', url, true);
	this.setRequestHeader('X-CSRFToken', '{{csrf_token}}');
	this.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
	xhr.send(data);
};
